local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

-- Configurações da Janela
local Window = OrionLib:MakeWindow({
    Name = "HAX STORE - Premium",
    HidePremium = false,
    IntroText = "Bem-vindo",
    SaveConfig = true,
    ConfigFolder = "HaxStoreConfigs",
    Icon = "rbxassetid://115939816657500"
})

-- Funções de Notificação
local function ShowNotification(title, content, duration)
    OrionLib:MakeNotification({
        Name = title,
        Content = content,
        Time = duration or 5,
        Image = "rbxassetid://115939816657500"
    })
end

-- Variáveis Globais
local autoCastEnabled = false
local autoShakeEnabled = false
local autoReelEnabled = false
local Noclip = false
local teleportSpots = {}
local autoShakeConnection
local autoReelConnection

-- Funções do Auto Shake
local function autoShake()
    local PlayerGui = game:GetService("Players").LocalPlayer:WaitForChild("PlayerGui")
    local shakeui = PlayerGui:FindFirstChild("shakeui")
    if not shakeui then return end
    local safezone = shakeui:FindFirstChild("safezone")
    local button = safezone and safezone:FindFirstChild("button")
    if button then
        game:GetService("VirtualInputManager"):SendMouseButtonEvent(button.AbsolutePosition.X + button.AbsoluteSize.X / 2, button.AbsolutePosition.Y + button.AbsoluteSize.Y / 2, 0, true, game)
        game:GetService("VirtualInputManager"):SendMouseButtonEvent(button.AbsolutePosition.X + button.AbsoluteSize.X / 2, button.AbsolutePosition.Y + button.AbsoluteSize.Y / 2, 0, false, game)
    end
end

local function startAutoShake()
    if autoShakeConnection then return end
    autoShakeConnection = game:GetService("RunService").RenderStepped:Connect(autoShake)
end

local function stopAutoShake()
    if autoShakeConnection then
        autoShakeConnection:Disconnect()
        autoShakeConnection = nil
    end
end

-- Funções do Auto Reel
local function autoReel()
    local PlayerGui = game:GetService("Players").LocalPlayer:WaitForChild("PlayerGui")
    local reel = PlayerGui:FindFirstChild("reel")
    if not reel then return end
    local bar = reel:FindFirstChild("bar")
    local playerbar = bar and bar:FindFirstChild("playerbar")
    local fish = bar and bar:FindFirstChild("fish")
    if playerbar and fish then
        playerbar.Position = fish.Position
    end
end

local function startAutoReel()
    if autoReelConnection then return end
    autoReelConnection = game:GetService("RunService").RenderStepped:Connect(autoReel)
end

local function stopAutoReel()
    if autoReelConnection then
        autoReelConnection:Disconnect()
        autoReelConnection = nil
    end
end

-- Tabs (Abas)
local MainTab = Window:MakeTab({
    Name = "Principal",
    Icon = "rbxassetid://6031075938",
    PremiumOnly = false
})

local ExclusiveTab = Window:MakeTab({
    Name = "Exclusivo",
    Icon = "rbxassetid://6031075953",
    PremiumOnly = true
})

local TeleportTab = Window:MakeTab({
    Name = "Teleporte",
    Icon = "rbxassetid://6031075954",
    PremiumOnly = false
})

local MiscTab = Window:MakeTab({
    Name = "Outros",
    Icon = "rbxassetid://6031075956",
    PremiumOnly = false
})

-- Elementos da Aba Principal
MainTab:AddButton({
    Name = "Executar Script Base",
    Callback = function()
        ShowNotification("HAX STORE", "Script executado com sucesso!", 5)
    end
})

MainTab:AddToggle({
    Name = "Auto Cast",
    Default = false,
    Callback = function(value)
        autoCastEnabled = value
        if value then
            ShowNotification("HAX STORE", "Auto Cast ativado!", 5)
        else
            ShowNotification("HAX STORE", "Auto Cast desativado!", 5)
        end
    end
})

MainTab:AddToggle({
    Name = "Auto Shake",
    Default = false,
    Callback = function(value)
        autoShakeEnabled = value
        if value then
            startAutoShake()
            ShowNotification("HAX STORE", "Auto Shake ativado!", 5)
        else
            stopAutoShake()
            ShowNotification("HAX STORE", "Auto Shake desativado!", 5)
        end
    end
})

MainTab:AddToggle({
    Name = "Auto Reel",
    Default = false,
    Callback = function(value)
        autoReelEnabled = value
        if value then
            startAutoReel()
            ShowNotification("HAX STORE", "Auto Reel ativado!", 5)
        else
            stopAutoReel()
            ShowNotification("HAX STORE", "Auto Reel desativado!", 5)
        end
    end
})

-- Elementos da Aba Exclusiva
ExclusiveTab:AddButton({
    Name = "Contador de Sombras",
    Callback = function()
        ShowNotification("HAX STORE", "Contador de Sombras ativado!", 5)
    end
})

ExclusiveTab:AddButton({
    Name = "Destruir Sombras",
    Callback = function()
        for _, shadow in pairs(workspace.Shadows:GetChildren()) do
            shadow:Destroy()
        end
        ShowNotification("HAX STORE", "Sombras destruídas!", 5)
    end
})

ExclusiveTab:AddSlider({
    Name = "Delay de Rod Dupe",
    Min = 0,
    Max = 1,
    Default = 0.2,
    Increment = 0.1,
    Callback = function(value)
        ShowNotification("HAX STORE", "Delay ajustado para: " .. value, 5)
    end
})

-- Elementos da Aba Teleporte
TeleportTab:AddDropdown({
    Name = "Selecionar Área",
    Default = "",
    Options = {"Aurora", "Sundial", "Windset", "Smokescreen", "Tempest"},
    Callback = function(selected)
        if selected == "Aurora" then
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-1811, -137, -3282)
        elseif selected == "Sundial" then
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-1148, 135, -1075)
        elseif selected == "Windset" then
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(2849, 178, 2702)
        elseif selected == "Smokescreen" then
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(2789, 140, -625)
        elseif selected == "Tempest" then
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(35, 133, 1943)
        end
        ShowNotification("HAX STORE", "Teleportado para: " .. selected, 5)
    end
})

TeleportTab:AddButton({
    Name = "Criar Zona Segura",
    Callback = function()
        local SafeZone = Instance.new("Part")
        SafeZone.Size = Vector3.new(30, 1, 30)
        SafeZone.Position = Vector3.new(math.random(-2000,2000), math.random(50000,90000), math.random(-2000,2000))
        SafeZone.Anchored = true
        SafeZone.BrickColor = BrickColor.new("Bright purple")
        SafeZone.Material = Enum.Material.ForceField
        SafeZone.Parent = game.Workspace
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = SafeZone.CFrame + Vector3.new(0, 5, 0)
        ShowNotification("HAX STORE", "Zona Segura criada!", 5)
    end
})

})
MiscTab:AddSlider({
    Name = "Velocidade de Movimento",
    Min = 16,
    Max = 200,
    Default = 16,
    Increment = 1,
    Callback = function(value)
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = value
        ShowNotification("HAX STORE", "Velocidade ajustada para: " .. value, 5)
    end
})

MiscTab:AddSlider({
    Name = "Altura do Pulo",
    Min = 50,
    Max = 200,
    Default = 50,
    Increment = 1,
    Callback = function(value)
        game.Players.LocalPlayer.Character.Humanoid.JumpPower = value
        ShowNotification("HAX STORE", "Altura do pulo ajustada para: " .. value, 5)
    end
})

MiscTab:AddToggle({
    Name = "Noclip",
    Default = false,
    Callback = function(value)
        Noclip = value
        if value then
            ShowNotification("HAX STORE", "Noclip ativado!", 5)
        else
            ShowNotification("HAX STORE", "Noclip desativado!", 5)
        end
    end
})

MiscTab:AddButton({
    Name = "Copiar Posição XYZ",
    Callback = function()
        local XYZ = tostring(game.Players.LocalPlayer.Character.HumanoidRootPart.Position)
        setclipboard("game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(" .. XYZ .. ")")
        ShowNotification("HAX STORE", "Posição copiada para a área de transferência!", 5)
    end
})

-- Inicialização
OrionLib:Init()

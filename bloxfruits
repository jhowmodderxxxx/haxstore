-- Initialization
function init()
    -- Find base address
    gg.searchNumber("999.765625", gg.TYPE_FLOAT)
    local results = gg.getResults(1)
    if #results > 0 then
        BASE_ADDRESS = results[1].address
        gg.clearResults()
    else
        gg.alert("Failed to find base address!")
        os.exit()
    end

    while true do
        if gg.isVisible() then
            gg.setVisible(false)
            menufarm()
        end
        gg.sleep(50)
    end
end

-- Start the script
init()
